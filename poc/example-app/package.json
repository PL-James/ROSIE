{
  "name": "@rosie/example-app",
  "version": "1.0.0",
  "description": "ROSIE Reference Implementation - Demo Application",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "test:watch": "vitest",
    "rosie:scan": "cd ../rosie-cli && npx tsx src/index.ts scan",
    "rosie:sync": "cd ../rosie-cli && npx tsx src/index.ts sync --sor-url http://localhost:3000"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^20.11.0",
    "typescript": "^5.3.3",
    "vitest": "^1.6.1"
  }
}
